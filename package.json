{
  "name": "jekyll-vue-template",
  "version": "2.0.0",
  "description": "A starter template for Jekyll projects with Vue.js and Vue Single File Components, complete with webpack.",
  "main": "index.js",
  "scripts": {
    "install-all": "bundle install & npm install",
    "update-all": "bundle update & npm update",
    "jekyll": "bundle exec jekyll serve --watch --incremental",
    "start": "cross-env NODE_ENV=development webpack --watch",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "keywords": [
    "jekyll",
    "vue",
    "ssg"
  ],
  "author": "Christopher Murphy",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.24.0",
    "fslightbox": "^3.3.0-2",
    "vue": "^2.6.11",
    "vue-cal": "^3.10.1",
    "vue-load-image": "^0.2.0",
    "vue-zoomer": "^0.3.9",
    "vuetify": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.7",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "vue-loader": "^15.9.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
